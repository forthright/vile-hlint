{
  "name": "@forthright/vile-hlint",
  "version": "0.2.1",
  "description": "A vile plugin for hlint",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel -d lib -m common src",
    "compile-dev": "coffee -cbo .test test",
    "dev": "bin/dev",
    "test": "bin/test",
    "vile": "vile p -usi --nodecorations",
    "release-it": "release-it"
